{
  "name": "@onigeya/bpmn-editor",
  "version": "0.1.0",
  "private": false,
  "description": "一个基于 Vue 3 开发的轻量级、可扩展的 BPMN 流程图编辑器组件",
  "keywords": [
    "bpmn",
    "workflow",
    "process",
    "vue3",
    "editor",
    "diagram"
  ],
  "author": {
    "name": "onigeya",
    "url": "https://github.com/onigeya"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onigeya/bpmn-editor.git"
  },
  "bugs": {
    "url": "https://github.com/onigeya/bpmn-editor/issues"
  },
  "homepage": "https://github.com/onigeya/bpmn-editor#readme",
  "type": "module",
  "main": "dist/bpmn-editor.umd.js",
  "module": "dist/bpmn-editor.es.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/bpmn-editor.es.js",
      "require": "./dist/bpmn-editor.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build --config vite.lib.config.ts",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint": "run-s lint:*",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@antv/x6": "^2.18.1",
    "@antv/x6-vue-shape": "^2.1.2",
    "@element-plus/icons-vue": "^2.3.1",
    "bpmn-moddle": "^9.0.1",
    "camunda-bpmn-moddle": "^7.0.1",
    "element-plus": "^2.9.7",
    "pinia": "^3.0.1",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@playwright/test": "^1.51.1",
    "@tsconfig/node22": "^22.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.14",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/eslint-plugin": "^1.1.38",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.23.0",
    "eslint-plugin-oxlint": "^0.15.15",
    "eslint-plugin-playwright": "^2.2.0",
    "eslint-plugin-vue": "~10.0.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^7.0.2",
    "oxlint": "^0.15.15",
    "prettier": "3.5.3",
    "sass-embedded": "^1.86.0",
    "terser": "^5.39.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.3",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitest": "^3.0.9",
    "vue-tsc": "^2.2.8"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "peerDependencies": {
    "element-plus": "^2.6.0",
    "vue": "^3.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
